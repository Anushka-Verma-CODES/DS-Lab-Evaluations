#include <iostream>
#include <cmath>
using namespace std;

int main() {
    const double g = 9.81;
    const double v0 = 25.0;
    const double dt = 0.1;
    const int maxSteps = 100;

    double x[maxSteps], y[maxSteps];

    for (int angle = 20; angle <= 80; angle += 5) {
        double rad = angle * M_PI / 180.0;
        double v0x = v0 * cos(rad);
        double v0y = v0 * sin(rad);
        double t_flight = (2 * v0y) / g;
        int steps = (int)(t_flight / dt) + 1;

        cout << "\nAngle = " << angle << " degrees\n";
        cout << "X(m)\tY(m)\n";

        for (int i = 0; i < steps && i < maxSteps; i++) {
            double t = i * dt;
            x[i] = v0x * t;
            y[i] = v0y * t - 0.5 * g * t * t;
            if (y[i] < 0) y[i] = 0;
            cout << x[i] << "\t" << y[i] << "\n";
        }
    }
    return 0;
}
